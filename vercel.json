{
    "version": 2,
    "builds": [
        {
            "src": "vercelHandler.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)", 
            "dest": "vercelHandler.ts" 
        },
        {
            "src": "/(.*)",
            "dest": "vercelHandler.ts"
        }
    ]
}




